<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:innerRadiusRatio="2.05"
            android:shape="ring"
            android:thickness="@dimen/dp2"
            android:useLevel="false">
            <solid android:color="#C95757" />
            <padding
                android:bottom="@dimen/dp8"
                android:left="@dimen/dp8"
                android:right="@dimen/dp8"
                android:top="@dimen/dp8" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/key_light_gray" />
        </shape>
    </item>

</layer-list>